    <arg name="gain_p"  default="0.15"/>
    <arg name="gain_i"  default="0.0"/>
    <arg name="gain_d"  default="0.0"/>
    <arg name="theta_lidar"  default="42"/>
    <arg name="dynamic_distance"  default="false"/>
